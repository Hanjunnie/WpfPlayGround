<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- 기본 공식 NuGet -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />

    <!-- DevExpress 피드 (API Key는 Actions Secrets에서 주입) -->
    <add key="DevExpress" value="https://nuget.devexpress.com/${DEVEXPRESS_NUGET_KEY}/api" />

    <!-- 로컬 개발자용 (있으면 자동 무시) -->
    <add key="DevExpress 24.1 Local" value="C:\Program Files\DevExpress 24.1\Components\System\Components\Packages" />
    <add key="DevExpress 24.2 Local" value="C:\Program Files\DevExpress 24.2\Components\System\Components\Packages" />
  </packageSources>

  <disabledPackageSources />
</configuration>
